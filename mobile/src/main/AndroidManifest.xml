<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.chaos.starke" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/application"
        android:name="com.orm.SugarApp"
        android:theme="@style/Theme.Starke"
        tools:replace="android:label">

        <meta-data android:name="DATABASE" android:value="starke.db" />
        <meta-data android:name="VERSION" android:value="2" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.chaos.starke" />

        <activity
            android:name=".core.MainActivity"
            android:label="@string/application"
            android:icon="@drawable/ic_dumbbell">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>
